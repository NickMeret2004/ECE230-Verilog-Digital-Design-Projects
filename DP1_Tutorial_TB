module tutorial_tb;
    reg A, B, C;
    wire X, Y;
    tutorial inst0(.a(A), .b(B), .c(C), .x(X), .y(Y));
    initial
    begin
        #0 A=0; B=0; C=0;
        #100 A=0; B=0; C=1;
        #100 A=0; B=1; C=0;
        #100 A=1; B=0; C=0;
        #100 A=0; B=1; C=1;
        #100 A=1; B=0; C=1;
        #100 A=1; B=1; C=0;
        #100 A=1; B=1; C=1;
    end
endmodule
